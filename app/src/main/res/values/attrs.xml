<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- TAG布局 -->
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="CountDownButton">
        <attr name="millisInFuture" format="integer" />
        <attr name="countDownInterval" format="integer" />
    </declare-styleable>
    
    <declare-styleable name="SearchEditText">
        <attr name="mode" format="integer"/>
        <attr name="hint" format="string"/>
        <attr name="searchIndexLength" format="integer"/>
        <attr name="searchable" format="boolean"/>
    </declare-styleable>
    
    <declare-styleable name="LimitSizeEditText">
        <attr name="maxLength" format="integer"/>
        <attr name="currentCountColor" format="integer"/>
        <attr name="splitColor" format="integer"/>
        <attr name="totalCountColor" format="integer"/>
        <attr name="minHeight" format="integer"/>
        <attr name="split" format="string"/>
        <attr name="isLimit" format="boolean"/>

    </declare-styleable>

</resources>